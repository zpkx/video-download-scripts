{
  "format": "bestvideo[height>=1080]+bestaudio/best",
  "merge_output_format": "mp4",
  "writeinfojson": true,
  "writesubtitles": true,
  "writeautomaticsub": true,
  "subtitleslangs": [ "zh-CN", "zh-TW", "en" ],
  "embed_subs": true,
  "writedesctription": true,
  "writethumbnail": true,
  "ignoreerrors": true,
  "extract_flat": false,
  "playlistend": 50,
  "geo_bypass": true,
  "retries": 3,
  "fragment_retries": 3,
  "skip_unavailable_fragments": true,
  "keep_fragments": false,
  "postprocessors": [
    {
      "key": "FFmpegVideoConvertor",
      "preferedformat": "mp4"
    },
    {
      "key": "EmbedSubtitle"
    },
    {
      "key": "FFmpegMetadata"
    }
  ]
}
